Algoritmo "VELOCIDADE_MEDIA"
Var
   distancia,horas,velocidade:real
Inicio
   escreva("Informe a distância que o carro percorreu:")
   leia(distancia)
   escreva("Informe o tempo que ele levou pra percorrer essa distância:")
   leia(horas)
   velocidade <- distancia/horas
   escreva("A média de velocidade foi de ",velocidade,"km por hora")
Fimalgoritmo

