Algoritmo "Area_Perimetro_Retangulo"
Var
   altura, largura, area, perimetro:real
Inicio
   escreva("Informe a altura do retângulo:")
   leia(altura)
   escreva("Informe a largura do retângulo:")
   leia(largura)
   area <- altura*largura
   perimetro <- 2*(altura+largura)
   escreva("A área é:",area,"! E o perímetro é:",largura,"!")
Fimalgoritmo

